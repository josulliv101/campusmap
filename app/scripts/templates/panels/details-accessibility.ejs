<div class="panel-bd panel-secondary">
	<div class="bd spacing">
		<h3 class="subhead">Accessibility</h3>
		<% if (model.location.accessibility > 0 || model.location.accessiblerestroom > 0) { %>
			<ul class="addr accessibility">
				<li>
				<% if (model.location.accessibility === 1) { %>
					<img src="./app/images/icons/accessibility1@2x.png" /> Fully Accessible
				<% } else if (model.location.accessibility === 2) { %>
					<img src="./app/images/icons/accessibility2@2x.png" /> Accessible with Assistance
				<% } %>
				</li>
				<% if (model.location.accessibilityrestroom === 1) { %>
					<li><img src="./app/images/icons/accessibilityrestroom@2x.png" /> Accessible Restroom</li>
				<% } %>
			</ul>
			<div class='footer'>
		<% } else { %>
			<div class='footer no-content'>
		<% } %>
			<a class="link external" href="http://oeo.tufts.edu/">Office of Equal Opportunity</a> 
			<% if (model.campus.campusid === 'medford') { %>
			<span class="divider">|</span> <a class="link external" target="_blank" href="http://campusmaps.tufts.edu/docs/medford-accessibilitymap.pdf">Accessibility Map (pdf)</a>
			<% } %>
			<button class="btn-close" data-campusmap='panels:details'>close</button>
		</div>
	</div>
</div>