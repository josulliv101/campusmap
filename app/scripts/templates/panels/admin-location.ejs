<div class="panel-bd panel-secondary">
	<div class="bd spacing">
		<button data-campusmap="panels:admin-add-locations">Add more locations</button>
		<form class="form-horizontal" role="form">
			<div class="form-group">
				<label for="inputId" class="col-sm-4 control-label">Location Id</label>
				<div class="col-sm-8">
					<input class="form-control" id="inputId" placeholder="Location Id" value="${model.location.locationid}">
				</div>
			</div>
			<div class="form-group">
				<label for="inputName" class="col-sm-4 control-label">Name</label>
				<div class="col-sm-8">
					<input class="form-control" id="inputName" placeholder="Location Name" value="${model.location.name}">
				</div>
			</div>
			<div class="form-group">
				<label for="inputEmphasis" class="col-sm-4 control-label">Emphasis</label>
				<div class="col-sm-8">
					<input class="form-control" id="inputEmphasis" placeholder="Emphasis" value="${model.location.emphasis}">
				</div>
			</div>
			<div class="form-group">
				<label for="inputLatLng" class="col-sm-4 control-label">LatLng</label>
				<div class="col-sm-8">
					<input class="form-control" id="inputLatLng" placeholder="Latitude/Longitude" value="${model.location.latlng}">
				</div>
			</div>
			<div class="form-group">
				<label for="inputLabelPlacement" class="col-sm-4 control-label">Label Placement</label>
				<div class="col-sm-8">
					<input class="form-control" id="inputLabelPlacement" placeholder="" value="${model.location.labelplacement}">
				</div>
			</div>
			<div class="form-group">
				<label for="inputTags" class="col-sm-4 control-label">Tags</label>
				<div class="col-sm-8">
					<input class="form-control" id="inputTags" placeholder="" value="${model.location.tags}">
				</div>
			</div>
			<div class="form-group">
				<label for="inputImage" class="col-sm-4 control-label">Image</label>
				<div class="col-sm-8">
					<input class="form-control" id="inputImage" placeholder="Image Name" value="${model.location.imageurl}">
				</div>
			</div>
			<div class="form-group">
				<div class="col-sm-offset-2 col-sm-8">
					<button type="submit" class="btn btn-default btn-save">Save</button>
				</div>
			</div>
		</form>
	</div>
</div>