{
  "curly": false,
  "es5": false,
  "expr": true,
  "eqeqeq": true,
  "immed": true,
  "latedef": true,
  "laxcomma": true,
  "newcap": true,
  "noarg": true,
  "sub": true,
  "undef": true,
  "unused": false,
  "boss": true,
  "eqnull": true,
  "browser": true,
  "strict": false,
  "predef": ["require", "module", "define", "alert", "expect", "console", "beforeEach", "google", "Modernizr"],
  "globals"   : {
    "describe"   : false,
    "it"         : false,
    "before"     : false,
    "beforeEach" : false,
    "after"      : false,
    "afterEach"  : false
  }
}
